{
	"name": "count",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "wrangler dev src/index.ts --port 7817 --env local",
		"deploy": "wrangler deploy --minify src/index.ts",
		"init:database:local": "wrangler d1 execute visitor-counter --local --file=./schema.sql --env local",
		"init:redis:local": "docker run -d -p 6379:6379 --name redis redis:alpine",
		"init:redis:srh:local": "docker run -it -d -p 15384:80 --add-host=host.docker.internal:host-gateway --name srh -e SRH_MODE=env -e SRH_TOKEN=HELLOWORLD -e SRH_CONNECTION_STRING=redis://host.docker.internal:6379 hiett/serverless-redis-http:latest",
		"check": "biome check",
		"format": "biome format",
		"lint": "biome lint"
	},
	"dependencies": {
		"@upstash/redis": "^1.35.6",
		"hono": "^4.10.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.7",
		"@cloudflare/workers-types": "^4.20251121.0",
		"effect": "3.19.6",
		"typescript": "^5.9.3",
		"wrangler": "^4.50.0"
	},
	"packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
